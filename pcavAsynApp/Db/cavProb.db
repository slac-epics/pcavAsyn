{
    field(DESC, "mean DCFreq for NCO Ctrl for Cav$(CAV) Prb$(PRB)")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),0) mean_dcfreq_nco_cav$(CAV)P$(PRB)")
