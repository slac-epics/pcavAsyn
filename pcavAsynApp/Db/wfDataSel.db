#
#  waveform data selectors

record(mbbo, "$(DEVICE):WFDATASEL0")
{
    field(PINI, "YES")
    field(DESC, "Waveform Data Selector 0")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) wfDataSel0")
    field(SCAN, "Passive")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(ZRST, "Cav0 NCO")
    field(ONST, "Cav1 NCO")
    info(autosaveFields, "VAL")
}


